{
  "name": "schitecore-managed-paas-10",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "convert": "pushd out; pandoc -s managed-cloud-10-paas.md -o managed-cloud-10-paas.docx; popd;",
    "convert-sla": "pushd out-sla; pandoc -s managed-cloud-10-paas-sla.md -o managed-cloud-10-paas-sla.docx; popd;",
    "start": "ts-node --files ./src/index.ts",
    "start-sla": "ts-node --files ./src/sla-pages/index.ts",
    "sign-chromium": "sudo codesign --force --deep --sign - ./node_modules/puppeteer/.local-chromium/mac-*/chrome-mac/Chromium.app",
    "eslint": "eslint \"src/**/*.{ts,js}\""
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "fs-extra": "^10.0.0",
    "joplin-turndown-plugin-gfm": "^1.0.12",
    "lodash": "^4.17.21",
    "puppeteer": "^13.2.0",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.16",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
